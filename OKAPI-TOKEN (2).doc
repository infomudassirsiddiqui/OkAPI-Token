    <p>&nbsp; &nbsp; &nbsp; &nbsp; async function connectWallet() {</p>
    <p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; try {</p>
    <p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; if (typeof window.ethereum !== &apos;undefined&apos;) {</p>
